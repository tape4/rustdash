# Environment variables for Monitoring Dashboard
# Copy this file to .env and modify as needed

# Prometheus Configuration
MONITOR_PROMETHEUS_BASE_URL=http://localhost:9090
MONITOR_PROMETHEUS_TIMEOUT_SECONDS=10

# Loki Configuration
MONITOR_LOKI_BASE_URL=http://localhost:3100
MONITOR_LOKI_TIMEOUT_SECONDS=10
MONITOR_LOKI_LOG_LIMIT=100

# UI Configuration
MONITOR_UI_REFRESH_INTERVAL_SECONDS=5
MONITOR_UI_LOG_DISPLAY_COUNT=20